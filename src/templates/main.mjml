<mjml>
    <mj-head>
        <mj-breakpoint width="600px"/>
        <mj-attributes>
            <mj-all color="inherit" font-family="Arial, sans-serif" font-size="16px" />
            <mj-button font-size="20px" font-weight="700" border-radius="0px" inner-padding="20px 20px" css-class="button" />
        </mj-attributes>
        <mj-raw>
            <meta name="color-scheme" content="light dark">
            <meta name="supported-color-schemes" content="light dark">
            <meta name="format-detection" content="telephone=no">
        </mj-raw>
        <mj-style>
            div[data-container] {
                /* Email builder compatability styles */
                max-width: 600px;
                margin: 0 auto;
            }

            body {
                color: #000000;
                font-family: Arial, sans-serif;
                font-size: 16px;
            }

            a {
                color: #094D9A;
            }

            h1,h2,h3,h4,h5,h6 {
                color: #000000;
                font-family: Arial, sans-serif;
            }

            h1 {
                font-size: 32px;
                line-height: 1.1;
            }

            h2 {
                font-size: 28px;
                line-height: 1.2;
            }

            h3 {
                font-size: 22px;
                line-height: 1.4;
            }

            h4 {
                font-size: 18px;
                line-height: 1.6;
            }

            p {
                font-size: 16px;
                line-height: 1.7;
            }

            .footer-text p {
                font-size: 14px;
            }

            .wysiwyg > div > *:first-child {
                margin-top: 0;
            }

            .wysiwyg > div > *:last-child {
                margin-bottom: 0;
            }

            .mobile-only {
                display: none!important;
            }

            @media (max-width: 599px) {
                .desktop-only {
                    display: none!important;
                }

                .signature-img {
                    width: 135px!important;
                }

                .signature-text {
                    width: calc(100% - 135px)!important;
                }

                .pre-footer-block .socials {
                  padding-left: 0!important;
                  padding-right: 12px!important;
                }

                .two-col-column > table > tbody > tr > td {
                    padding: 0!important;
                }

                .pre-footer-text > table > tbody > tr > td {
                    padding-right: 0!important;
                }

                .pre-footer-block .social-link > td {
                    padding: 0 6px!important;
                }
            }
        </mj-style>
        <mj-style>
            /* dark mode styles */
            :root {
                color-scheme: light dark;
                supported-color-schemes: light dark;
            }

            .dark-only {
                display: none;
            }

            [data-ogsc] .dark-only {
                display: block !important;
            }

            [data-ogsc] .light-only {
                display: none !important;
            }


            @media (prefers-color-scheme: dark) {
                .light-only {
                    display: none !important;
                }

                .dark-only {
                    display: block !important;
                }

                body,
                .email-body {
                    color: #eeeeee !important;
                    background-color: #171819 !important;
                }

                a {
                    color: #9ca4a5 !important;
                }

                h1,h2,h3,h4,h5,h6 {
                    color:  #b0b4b5 !important;
                }

                .footer-block p {
                    color: #eeeeee !important;
                }

                .block,
                .block > table,
                .spacer-block td {
                    background-color: #0e0f0f !important;
                }

                .image-text-button-block.block,
                .image-text-button-block.block > table,
                .two-col-image-text-button-block.block,
                .two-col-image-text-button-block.block > table {
                    background-color: #1f2021  !important;
                }

                .button td,
                .button a {
                    background-color: #eaebec !important;
                    border-color: #eaebec !important;
                    color: #121314 !important;
                }

                .social-link table {
                    background-color: #0e0f0f  !important;
                }

                .social-link table td {
                    padding: 1px !important;
                }

                .divider > p {
                    border-color: #eeeeee!important;
                }
            }
        </mj-style>
        <mj-style>
            /* sizing classes for adjustable column widths 50-550px */
            @media only screen and (min-width:600px) {
                .mj-column-px-50,
                .moz-text-html .mj-column-px-50 {
                    width: 50px !important;
                    max-width: 50px;
                }

                .mj-column-px-100,
                .moz-text-html .mj-column-px-100 {
                    width: 100px !important;
                    max-width: 100px;
                }

                .mj-column-px-150,
                .moz-text-html .mj-column-px-150 {
                    width: 150px !important;
                    max-width: 150px;
                }

                .mj-column-px-200,
                .moz-text-html .mj-column-px-200 {
                    width: 200px !important;
                    max-width: 200px;
                }

                .mj-column-px-250,
                .moz-text-html .mj-column-px-250 {
                    width: 250px !important;
                    max-width: 250px;
                }

                .mj-column-px-300,
                .moz-text-html .mj-column-px-300 {
                    width: 300px !important;
                    max-width: 300px;
                }

                .mj-column-px-350,
                .moz-text-html .mj-column-px-350 {
                    width: 350px !important;
                    max-width: 350px;
                }

                .mj-column-px-400,
                .moz-text-html .mj-column-px-400 {
                    width: 400px !important;
                    max-width: 400px;
                }

                .mj-column-px-450,
                .moz-text-html .mj-column-px-450 {
                    width: 450px !important;
                    max-width: 450px;
                }

                .mj-column-px-500,
                .moz-text-html .mj-column-px-500 {
                    width: 500px !important;
                    max-width: 500px;
                }

                .mj-column-px-550,
                .moz-text-html .mj-column-px-550 {
                    width:550px !important;
                    max-width: 550px;
                }
            }
            @media only screen and (max-width:599px) {
                table.mj-full-width-mobile {
                    width: 100% !important;
                }

                td.mj-full-width-mobile {
                    width: auto !important;
                }

                td.mj-full-width-mobile img {
                    height: auto!important;
                }
            }
        </mj-style>
        <mj-raw>
            <!-- Thunderbird and other clients responsive CSS -->
            <style media="screen and (min-width:600px)">
              .moz-text-html .mj-column-per-100 {
